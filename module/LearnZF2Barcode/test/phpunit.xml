<phpunit bootstrap="Bootstrap.php" colors="true">
    <testsuite name="LearnZF2Ajax" >
        <directory>./LearnZF2BarcodeTest</directory>
    </testsuite>
    <filter>
        <whitelist>
            <directory suffix=".php">./../</directory>
            <exclude>
                <directory suffix=".php">./../test/</directory>
            </exclude>
        </whitelist>
        <blacklist>
            <directory suffix=".php">./../../../vendor/</directory>
        </blacklist>
    </filter>
    <logging>
        <log type="coverage-html" target="./../../../data/test-html-report/learnzf2barcode/" charset="UTF-8" yui="true" />
        <log type="coverage-php" target="./../../../build/coverage/coverage-learnzf2barcode.cov"/>
    </logging>
</phpunit>
